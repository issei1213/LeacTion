{
  "name": "leaction",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "tsc --noEmit && NODE_ENV=production vite build",
    "serve": "(cd functions/ && tsc) && firebase serve",
    "watch": "vite build --watch",
    "test": "jest",
    "format": "eslint -c .eslintrc.json --fix src/**/*.{ts,tsx}",
    "emulate": "yarn build_function_for_emulator && yarn build_for_emulator && firebase emulators:start & yarn watch",
    "build_for_emulator": "tsc --noEmit && vite build --mode emulator",
    "build_function_for_emulator": "(cd functions/ && npm run build)",
    "deploy": "firebase deploy"
  },
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-solid": "^0.7.0",
    "firebase": "^9.8.3",
    "firebase-tools": "^11.1.0",
    "prettier": "^2.7.1",
    "typescript": "^4.6.4",
    "vite": "^2.9.9",
    "vite-plugin-solid": "^2.2.6"
  },
  "dependencies": {
    "@solidjs/router": "^0.4.2",
    "@suid/icons-material": "^0.2.4",
    "@suid/material": "^0.4.2",
    "solid-app-router": "^0.4.1",
    "solid-js": "^1.4.2"
  }
